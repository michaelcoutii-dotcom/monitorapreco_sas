# Frontend Environment Variables
# .env.local ou .env.development.local para sobrescrever

# Backend API URL (Spring Boot)
# Default: http://localhost:8081
# Para produção: https://seu-backend.com
VITE_API_URL=http://localhost:8081

# Scraper API URL (Python)
# Usado internamente pelo Backend
# Default: http://localhost:8000
VITE_SCRAPER_URL=http://localhost:8000

# Log Level
VITE_LOG_LEVEL=debug
